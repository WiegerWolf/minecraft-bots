{
  "name": "minecraft-bots",
  "version": "1.0.0",
  "description": "Minecraft bot using Mineflayer",
  "main": "src/bot.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/bot.ts",
    "dev:manager": "bun run src/index.ts",
    "start:farmer": "BOT_ROLE=goap-farming BOT_NAME=DevFarmer bun run src/bot.ts",
    "start:lumberjack": "BOT_ROLE=goap-lumberjack BOT_NAME=DevLumberjack bun run src/bot.ts",
    "start:landscaper": "BOT_ROLE=landscaper BOT_NAME=DevLandscaper bun run src/bot.ts",
    "dev:farmer": "BOT_ROLE=goap-farming BOT_NAME=DevFarmer bun --watch src/bot.ts",
    "dev:lumberjack": "BOT_ROLE=goap-lumberjack BOT_NAME=DevLumberjack bun --watch src/bot.ts",
    "dev:landscaper": "BOT_ROLE=landscaper BOT_NAME=DevLandscaper bun --watch src/bot.ts",
    "postinstall": "patch-package"
  },
  "keywords": [
    "minecraft",
    "bot",
    "mineflayer"
  ],
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "mineflayer": "^4.33.0",
    "mineflayer-pathfinder": "^2.4.5",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "patch-package": "^8.0.1"
  }
}